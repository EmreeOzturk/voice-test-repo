{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/media-stream",
            "dest": "index.js",
            "headers": {
                "Upgrade": "websocket",
                "Connection": "upgrade"
            }
        },
        {
            "src": "/incoming-call",
            "dest": "index.js"
        },
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}